button {
  outline: none;
  border: none;
}
page {
  font-family: Monospace;
}
.tabBar {
  z-index: 100;
  width: 100%;
  height: 130rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  font-size: 28rpx;
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center center;
  color: #636363;
}
.cont {
  z-index: 0;
  display: flex;
  justify-content: space-between;
  item-align: center;
}
.cont .item:nth-child(3) image {
  width: 40rpx;
  height: 40rpx;
  padding: 10rpx;
  border-radius: 50%;
  background-color: #fff;
  transform: scale(1.2) translateY(-30rpx);
  background-color: #0497FF;
}
.cont .item:nth-child(3) .txt {
  margin-top: 4rpx;
}
.cont .item {
  font-size: 24rpx;
  position: relative;
  width: 15%;
  text-align: center;
  padding: 0;
  display: block;
  height: auto;
  line-height: 1;
  margin: 0;
  background-color: inherit;
  overflow: initial;
  justify-content: center;
  align-items: center;
  padding-top: 20rpx;
}
.cont .selected {
  color: #217EFD;
}
.cont image {
  width: 43rpx;
  height: 43rpx;
}
.cont .txt {
  margin-top: 20rpx;
}