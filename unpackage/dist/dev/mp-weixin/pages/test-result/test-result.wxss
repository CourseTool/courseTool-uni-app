button {
  outline: none;
  border: none;
}
page {
  font-family: Monospace;
}
.test-result {
  margin: 20rpx;
}
.test-result > view {
  margin: 20rpx 0;
}
.test-result > view view {
  font-size: 30rpx;
  color: #555;
}
.test-result > view view span {
  font-size: 40rpx;
  color: #000;
  margin-left: 10rpx;
  font-weight: bold;
}
.holland-result {
  margin: 20rpx;
}
.holland-result .header {
  padding: 20rpx;
  background-color: #ecedf4;
}
.holland-result .header .title {
  font-size: 40rpx;
  color: #000;
}
.holland-result .header .result {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.holland-result .header .result view {
  font-size: 30rpx;
  color: #0497FF;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.holland-result .header .result view span {
  margin: 0 10rpx;
  font-size: 40rpx;
}
.holland-result .scroll-view {
  height: 80rpx;
  padding: 20rpx;
  padding-left: 0;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
  margin: 20rpx 0;
  white-space: nowrap;
  display: flex;
  align-items: center;
  justify-content: center;
}
.holland-result .scroll-view .nav {
  width: -webkit-fit-content;
  width: fit-content;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.holland-result .scroll-view .nav .item {
  width: 150rpx;
  height: 40rpx;
  padding: 20rpx;
  margin-right: 20rpx;
  border-radius: 10rpx;
  background-color: #0497FF;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 40rpx;
}
.holland-result .content {
  margin: 20rpx;
  padding: 20rpx;
  background-color: #ecedf4;
}
.holland-result .content .title {
  color: #0497FF;
  font-size: 32rpx;
  text-align: center;
}
.holland-result .content .introduce {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.holland-result .content .introduce view {
  font-size: 32rpx;
  color: #222;
}
.holland-result .content .introduce view span {
  font-size: 30rpx;
  color: #666;
  font-weight: bold;
}
.holland-result .content .career {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.holland-result .content .career view {
  font-size: 32rpx;
  color: #222;
}
.holland-result .content .career view span {
  font-size: 30rpx;
  color: #666;
  font-weight: bold;
}
.retest {
  width: 230rpx;
  height: 80rpx;
  border-radius: 10rpx;
  background-color: #0497FF;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #fff;
  margin: 0 auto;
  margin-top: 30rpx;
  font-size: 30rpx;
}